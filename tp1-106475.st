Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas colegios'	classVariableNames: ''	package: 'tp1-106475'!!AlgoVid methodsFor: 'initialization' stamp: 'GonzaRE 5/17/2021 00:01'!initialize 	personas := OrderedCollection new.	burbujas := OrderedCollection new.	colegios := OrderedCollection new.	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 12:18'!clasesPresencialesEn: nombreColegio|colegio|colegio := self buscarColegio: nombreColegio.^ colegio hayClasesPresenciales.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/21/2021 17:34'!nuevaPersona: nombrePersona	|persona|	(self pertenece: nombrePersona En: personas)ifTrue:[		^ PersonaRepetida signal.		].	persona := Persona new.	persona establecerNombre: nombrePersona.		 	personas add: persona.	^persona.	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/16/2021 17:54'!diagnosticoDe: nombrePersona|persona|persona := self buscarPersona: nombrePersona.^ (persona obtenerDiagnostico).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/11/2021 19:38'!estaVacunadx: nombrePersona|persona|persona := self buscarPersona: nombrePersona.persona vacunar.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/21/2021 17:28'!pertenece: nombre En: coleccion^ coleccion anySatisfy: [ :objeto |	   objeto obtenerNombre == nombre	 ].! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 11:09'!burbujaPinchada: nombreBurbuja|burbuja|burbuja := self buscarBurbuja: nombreBurbuja.^ (burbuja estaPinchada).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/21/2021 17:35'!nuevoColegio: nombreColegio	|colegio|		(self pertenece: nombreColegio En: colegios)ifTrue:[		^ ColegioRepetido signal.		].	colegio := Colegio new.	colegio establecerNombre: nombreColegio.		 	colegios add: colegio.	^colegio.	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/17/2021 18:48'!puedeCircular: nombrePersona|persona|persona := self buscarPersona: nombrePersona.^ (persona puedeCircular).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/17/2021 18:22'!agregarSintoma: nombreSintoma a: nombrePersona|persona|persona := self buscarPersona: nombrePersona.persona personaAgregarSintoma: nombreSintoma.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/20/2021 20:46'!buscarBurbuja: nombreBurbuja^burbujas detect: [ :burbuja |	   (burbuja obtenerNombre = nombreBurbuja) 	 ] ifNone:[				BurbujaNoExiste signal.	] .! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/18/2021 19:25'!esPersonalEsencial: nombrePersona|persona|persona := self buscarPersona: nombrePersona.persona esPersonalEsencialPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 13:06'!agregarA: nombrePersona aLaBurbuja: nombreBurbuja|persona burbuja|persona := self buscarPersona: nombrePersona.burbuja := self buscarBurbuja: nombreBurbuja.(burbuja agregarA: persona).(persona agregarBurbujaALaQuePertenece: burbuja).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/18/2021 19:25'!esPersonaDeRiesgo: nombrePersona|persona|persona := self buscarPersona: nombrePersona.persona esPersonaDeRiesgoPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/21/2021 17:35'!nuevaBurbuja: nombreBurbuja	|burbuja|		(self pertenece: nombreBurbuja En: burbujas)ifTrue:[		^ BurbujaRepetida signal.		].	burbuja := Burbuja new.	burbuja establecerNombre: nombreBurbuja.		 	burbujas add: burbuja.	^burbuja.	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/20/2021 20:46'!buscarColegio: nombreColegio^colegios detect: [ :cole |	   (cole obtenerNombre = nombreColegio) 	 ] ifNone:[				ColegioNoExiste signal.	] .! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 12:13'!agregarA: nombreBurbuja alColegio: nombreColegio|burbuja colegio|burbuja := self buscarBurbuja: nombreBurbuja.colegio := self buscarColegio: nombreColegio.colegio agregarA: burbuja.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/13/2021 20:00'!contactoEstrechoEntre: nombrePersona1 y: nombrePersona2|persona1 persona2|persona1 := self buscarPersona: nombrePersona1.persona2 := self buscarPersona: nombrePersona2.persona1 contactoCon: persona2.persona2 contactoCon: persona1.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/20/2021 20:45'!buscarPersona: NombrePersona^personas detect: [ :pers |	   (pers obtenerNombre = NombrePersona) 	 ] ifNone:[		PersonaNoExiste signal.		] .	! !Object subclass: #Burbuja	instanceVariableNames: 'nombre personas'	classVariableNames: ''	package: 'tp1-106475'!!Burbuja methodsFor: 'initialization' stamp: 'GonzaRE 5/16/2021 15:37'!initialize personas := OrderedCollection new.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/18/2021 20:41'!establecerNombre: nombreBurbuja	nombre:= nombreBurbuja.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 13:52'!inhabilitarIntegrantespersonas do: [ :persona | persona estaEnBurbujaPinchada ]! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 11:03'!agregarA: persona personas add: persona.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 11:03'!obtenerNombre^nombre.! !!Burbuja methodsFor: 'accessing' stamp: 'GonzaRE 6/16/2021 17:54'!estaPinchada|pinchada|pinchada := personas anySatisfy: [ :persona |	   persona obtenerDiagnostico = 'Positivo'.	 ].^pinchada.! !Error subclass: #BurbujaNoExiste	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-106475'!Error subclass: #BurbujaRepetida	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-106475'!Object subclass: #Colegio	instanceVariableNames: 'nombre burbujas'	classVariableNames: ''	package: 'tp1-106475'!!Colegio methodsFor: 'initialization' stamp: 'GonzaRE 5/14/2021 12:17'!initialize burbujas := OrderedCollection new.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 12:11'!establecerNombre: nombreColegionombre:=nombreColegio .! !!Colegio methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 14:01'!hayClasesPresenciales^( (((self cantidadBurbujasPinchadas)*100)/(burbujas size))<40 ).! !!Colegio methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 11:56'!agregarA: burbuja burbujas add: burbuja.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 14:01'!cantidadBurbujasPinchadas|seleccionados|seleccionados:= burbujas select: [:burbuja | 		(burbuja estaPinchada).	].^seleccionados size.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/14/2021 11:53'!obtenerNombre^nombre.! !Error subclass: #ColegioNoExiste	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-106475'!Error subclass: #ColegioRepetido	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-106475'!Object subclass: #DeRiesgo	instanceVariableNames: 'estado'	classVariableNames: ''	package: 'tp1-106475'!!DeRiesgo methodsFor: 'initialization' stamp: 'GonzaRE 6/14/2021 12:06'!volverDeRiesgoestado:= true.! !!DeRiesgo methodsFor: 'initialization' stamp: 'GonzaRE 6/14/2021 12:06'!initialize estado := false.! !!DeRiesgo methodsFor: 'initialization' stamp: 'GonzaRE 6/17/2021 19:28'!esDeRiesgo^estado.! !Object subclass: #Esencial	instanceVariableNames: 'estado'	classVariableNames: ''	package: 'tp1-106475'!!Esencial methodsFor: 'initialization' stamp: 'GonzaRE 6/14/2021 12:08'!esEsencial^ estado.! !!Esencial methodsFor: 'initialization' stamp: 'GonzaRE 6/14/2021 12:09'!initialize estado := false.! !!Esencial methodsFor: 'initialization' stamp: 'GonzaRE 6/14/2021 12:09'!volverEsencialestado:= true.! !Object subclass: #EstadoSalud	instanceVariableNames: 'riesgo esencial vacunado sintomas estadocovid'	classVariableNames: ''	package: 'tp1-106475'!!EstadoSalud methodsFor: 'initialization' stamp: 'GonzaRE 6/20/2021 22:53'!initializesintomas := OrderedCollection new. riesgo := DeRiesgo new.vacunado := Vacunado new.esencial:= Esencial new.estadocovid := Sospechoso new.! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/21/2021 19:41'!agregarSintoma: nombreSintoma|sintoma|sintoma := Sintoma new.sintoma agregarNombre: nombreSintoma.sintoma clasificarSintoma.sintomas add: sintoma.self esSospechoso.! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/20/2021 22:31'!puedeCircular^(esencial esEsencial | vacunado estaVacunado ) & (riesgo esDeRiesgo) not & (estadocovid puedeCircular).! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/15/2021 21:28'!verCantSintomas^sintomas size.! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/16/2021 18:01'!darDiagnostico(sintomas size > 0)ifTrue: [ 				((sintomas size>3) | ((self haySintomaHabitual ) & (sintomas size >2)))ifTrue: [ 	self esPositivo .	^'Positivo'	.		].	self esSospechoso.	^'Sospechoso'.	].! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/15/2021 21:32'!haySintomaHabitual^ sintomas anySatisfy: [ :sintoma |	   sintoma esHabitual.	 ].! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/20/2021 22:32'!verSospechoso^ estadocovid esSospechoso. ! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/20/2021 22:32'!verPositivo^ estadocovid esPositivo. ! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/21/2021 19:46'!huboContactoCon: personaConLaQueHuboContacto	((personaConLaQueHuboContacto esSospechoso) | (personaConLaQueHuboContacto esPositivo ))ifTrue: [ 		self esSospechoso.		 ].! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/21/2021 19:51'!estoyEnBurbujaPinchada		self esSospechoso.		 ! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/15/2021 21:23'!vacunacionvacunado seVacuna.! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/17/2021 19:13'!esEsencialesencial volverEsencial.! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/20/2021 22:37'!esPositivoestadocovid := Positivo new.estadocovid volverPositiva.! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/15/2021 21:05'!esDeRiesgoriesgo volverDeRiesgo.! !!EstadoSalud methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/20/2021 22:52'!esSospechoso(estadocovid esPositivo not)ifTrue: [ estadocovid volverSospechosa.].! !Object subclass: #Persona	instanceVariableNames: 'nombre burbujasalasquepertenece estadosalud'	classVariableNames: ''	package: 'tp1-106475'!!Persona methodsFor: 'accessing' stamp: 'GonzaRE 6/21/2021 19:46'!contactoCon: personaConLaQueHuboContactoestadosalud huboContactoCon: personaConLaQueHuboContacto.			! !!Persona methodsFor: 'accessing' stamp: 'GonzaRE 6/17/2021 19:16'!esPersonalEsencialPersona	estadosalud esEsencial.		! !!Persona methodsFor: 'accessing' stamp: 'GonzaRE 6/17/2021 18:46'!obtenerDiagnostico (estadosalud darDiagnostico = 'Positivo')ifTrue: [ 					self inhabilitarConvivientesBurbuja.			^'Positivo'.				].	^'Sospechoso'.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/17/2021 19:08'!puedeCircular^ (estadosalud puedeCircular) & (self perteneceABurbujaPinchada) not.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/17/2021 18:45'!esPersonaDeRiesgoPersonaestadosalud esDeRiesgo.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/21/2021 19:40'!personaAgregarSintoma: nombreSintomaestadosalud agregarSintoma:nombreSintoma.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/17/2021 18:43'!perteneceABurbujaPinchada^ burbujasalasquepertenece anySatisfy: [ :burbuja |	   burbuja estaPinchada.	 ].! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/17/2021 18:47'!vacunarestadosalud vacunacion.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/15/2021 21:47'!obtenerCantSintomas^ estadosalud verCantSintomas.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/18/2021 20:40'!establecerNombre: NombrePersona	nombre:= NombrePersona.	! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/17/2021 18:00'!agregarBurbujaALaQuePertenece: burbuja burbujasalasquepertenece add: burbuja.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/21/2021 19:51'!estaEnBurbujaPinchadaestadosalud estoyEnBurbujaPinchada.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/17/2021 19:16'!initializeburbujasalasquepertenece := OrderedCollection new.estadosalud := EstadoSalud new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/10/2021 19:59'!obtenerNombre^nombre.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/15/2021 21:32'!poseeSintomaHabitual^ estadosalud haySintomaHabitual.! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/16/2021 22:08'!esPositivo^(estadosalud verPositivo).! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 5/17/2021 17:59'!inhabilitarConvivientesBurbuja	burbujasalasquepertenece do: [ :burbuja | burbuja inhabilitarIntegrantes ].	 ! !!Persona methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/16/2021 22:04'!esSospechoso^(estadosalud verSospechoso).! !Error subclass: #PersonaNoExiste	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-106475'!Error subclass: #PersonaRepetida	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-106475'!Object subclass: #Positivo	instanceVariableNames: 'estado'	classVariableNames: ''	package: 'tp1-106475'!!Positivo methodsFor: 'as yet unclassified' stamp: 'GonzaRE 6/17/2021 19:27'!esPositivo^ estado